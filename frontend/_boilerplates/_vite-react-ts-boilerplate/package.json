{
    "name": "@wbk--reactjs-playground--typescript/_vite-react-ts-boilerplate",
    "formattedName": "Vite React TypeScript Boilerplate",
    "description": "A Vite-based React TypeScript boilerplate with ESLint, Prettier, and Sass support.",
    "private": true,
    "version": "0.3.0",
    "type": "module",
    "scripts": {
        "develop:boilerplates": "vite",
        "build:dev:boilerplates": "export NODE_ENV=development && tsc && vite build",
        "build:prod:boilerplates": "export NODE_ENV=production && tsc && vite build",
        "lint:boilerplates": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview:boilerplates": "vite preview",
        "package:boilerplates": "bash scripts/package.bash"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.3.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "eslint": "^9.32.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "sass": "^1.69.5",
        "typescript": "^5.8.3",
        "vite": "^5.4.19"
    }
}
